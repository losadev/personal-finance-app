#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Pablo/Desktop/personal-finance-app/backend/node_modules/.pnpm/ts-jest@29.4.4_@babel+core@_b8e5eb2c5f669f74efa6f639b51ecec3/node_modules/ts-jest/node_modules:/mnt/c/Users/Pablo/Desktop/personal-finance-app/backend/node_modules/.pnpm/ts-jest@29.4.4_@babel+core@_b8e5eb2c5f669f74efa6f639b51ecec3/node_modules:/mnt/c/Users/Pablo/Desktop/personal-finance-app/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Pablo/Desktop/personal-finance-app/backend/node_modules/.pnpm/ts-jest@29.4.4_@babel+core@_b8e5eb2c5f669f74efa6f639b51ecec3/node_modules/ts-jest/node_modules:/mnt/c/Users/Pablo/Desktop/personal-finance-app/backend/node_modules/.pnpm/ts-jest@29.4.4_@babel+core@_b8e5eb2c5f669f74efa6f639b51ecec3/node_modules:/mnt/c/Users/Pablo/Desktop/personal-finance-app/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
