#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Pablo/Desktop/personal-finance-app/backend/node_modules/.pnpm/jest-cli@30.2.0_@types+node_fa6c3f5a639b3ae6dc5f22fe821f2674/node_modules/jest-cli/bin/node_modules:/mnt/c/Users/Pablo/Desktop/personal-finance-app/backend/node_modules/.pnpm/jest-cli@30.2.0_@types+node_fa6c3f5a639b3ae6dc5f22fe821f2674/node_modules/jest-cli/node_modules:/mnt/c/Users/Pablo/Desktop/personal-finance-app/backend/node_modules/.pnpm/jest-cli@30.2.0_@types+node_fa6c3f5a639b3ae6dc5f22fe821f2674/node_modules:/mnt/c/Users/Pablo/Desktop/personal-finance-app/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Pablo/Desktop/personal-finance-app/backend/node_modules/.pnpm/jest-cli@30.2.0_@types+node_fa6c3f5a639b3ae6dc5f22fe821f2674/node_modules/jest-cli/bin/node_modules:/mnt/c/Users/Pablo/Desktop/personal-finance-app/backend/node_modules/.pnpm/jest-cli@30.2.0_@types+node_fa6c3f5a639b3ae6dc5f22fe821f2674/node_modules/jest-cli/node_modules:/mnt/c/Users/Pablo/Desktop/personal-finance-app/backend/node_modules/.pnpm/jest-cli@30.2.0_@types+node_fa6c3f5a639b3ae6dc5f22fe821f2674/node_modules:/mnt/c/Users/Pablo/Desktop/personal-finance-app/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../jest-cli/bin/jest.js" "$@"
else
  exec node  "$basedir/../../../jest-cli/bin/jest.js" "$@"
fi
