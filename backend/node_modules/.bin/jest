#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Pablo/Desktop/personal-finance-app/backend/node_modules/.pnpm/jest@30.2.0_@types+node@22._f2a208d2a99f7e8f8fcc2f0d86f787b9/node_modules/jest/bin/node_modules:/mnt/c/Users/Pablo/Desktop/personal-finance-app/backend/node_modules/.pnpm/jest@30.2.0_@types+node@22._f2a208d2a99f7e8f8fcc2f0d86f787b9/node_modules/jest/node_modules:/mnt/c/Users/Pablo/Desktop/personal-finance-app/backend/node_modules/.pnpm/jest@30.2.0_@types+node@22._f2a208d2a99f7e8f8fcc2f0d86f787b9/node_modules:/mnt/c/Users/Pablo/Desktop/personal-finance-app/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Pablo/Desktop/personal-finance-app/backend/node_modules/.pnpm/jest@30.2.0_@types+node@22._f2a208d2a99f7e8f8fcc2f0d86f787b9/node_modules/jest/bin/node_modules:/mnt/c/Users/Pablo/Desktop/personal-finance-app/backend/node_modules/.pnpm/jest@30.2.0_@types+node@22._f2a208d2a99f7e8f8fcc2f0d86f787b9/node_modules/jest/node_modules:/mnt/c/Users/Pablo/Desktop/personal-finance-app/backend/node_modules/.pnpm/jest@30.2.0_@types+node@22._f2a208d2a99f7e8f8fcc2f0d86f787b9/node_modules:/mnt/c/Users/Pablo/Desktop/personal-finance-app/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jest/bin/jest.js" "$@"
else
  exec node  "$basedir/../jest/bin/jest.js" "$@"
fi
